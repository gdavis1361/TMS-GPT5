{
  "title": "API Overview",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "Request Rate",
      "targets": [
        { "expr": "sum(rate(http_requests_total[1m]))" }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 8, "h": 4 }
    },
    {
      "type": "gauge",
      "title": "p95 Latency (s)",
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))" }
      ],
      "gridPos": { "x": 8, "y": 0, "w": 8, "h": 4 }
    },
    {
      "type": "timeseries",
      "title": "Latency by status",
      "targets": [
        { "expr": "sum(rate(http_request_duration_seconds_sum[1m])) by (status) / sum(rate(http_request_duration_seconds_count[1m])) by (status)" }
      ],
      "gridPos": { "x": 0, "y": 4, "w": 16, "h": 8 }
    }
  ]
}

